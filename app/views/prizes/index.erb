<div class="display">
    <h1>All Prizes</h1>
        <% @user.prizes.each do |prize| %>
            <%= Horse.find(prize.horse_id).name%>: <%= prize.point_total %>
            <button><a href="/prizes/<%= prize.id %>">More Info</a></button><br>
        <% end %>
    <br>
    <button><a href="/prizes/new">Add Prize</a></button><br>
</div>