<nav>
    <button class="home"><a href="/"><strong>Home</strong></a></button>
    <button class="logout"><a href="/logout"><strong>Logout</strong></a></button>
</nav>

<div class="display">
    <h1>All Prizes</h1>
        <% sorted_horseshows.each do |horseshow| %>
            <h2><%= horseshow.date %>: <%= horseshow.name %></h2>
                <% horseshow.prizes.each do |prize| %>
                    <a href="/prizes/<%= prize.id %>"><%= Horse.find_by(id: prize.horse_id).name%>: <%= prize.point_total %> points</a><br>
                <%end%>
            <% end %>
    <br>
    <button><a href="/prizes/new">Add Prize</a></button><br>
</div>